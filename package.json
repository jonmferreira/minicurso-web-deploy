{
  "name": "projeto-base",
  "private": true,
  "version": "0.0.1",
  "homepage": "https://jonmferreira.github.io/minicurso-web-deploy/",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 8081",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview  --port 8081",
    "build-image-docker": "docker build -t nomde_da_imagem .",
    "run-image-docker": "docker run -d --rm -p 8081:8081 --name nomde_da_container nomde_da_imagem",
    "abrir-log-container": "docker logs -f -t nomde_da_container",
    "test": "jest",
    "testv": "vitest",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "postinstall": "npm run build",
    "start" : "node server.js"
  },
  "engines": {
    "node": "18.16.0",
    "npm": "9.6.6"
  },
  "dependencies": {
    "@hookform/resolvers": "3.1.0",
    "@react-pdf/renderer": "3.1.13",
    "axios": "1.4.0",
    "moment": "2.29.4",
    "primeflex": "3.3.0",
    "primeicons": "6.0.1",
    "primereact": "9.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.9",
    "react-icons": "4.8.0",
    "react-router-dom": "6.11.2",
    "vite-tsconfig-paths": "4.2.0",
    "zod": "3.21.4",
    "zustand": "4.4.2"
  },
  "devDependencies": {
    "@jest/globals": "29.6.2",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.3",
    "@types/node": "20.4.0",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "@vitejs/plugin-react": "4.0.0",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "@vitejs/plugin-react-swc": "3.3.2",
    "@vitest/coverage-v8": "0.34.4",
    "axios-mock-adapter": "1.21.5",
    "babel-jest": "29.6.2",
    "eslint": "8.40.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.3.5",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "jest-extended": "4.0.1",
    "msw": "^1.2.3",
    "regenerator-runtime": "0.14.0",
    "ts-jest": "29.1.1",
    "typescript": "5.0.4",
    "vite": "4.3.9",
    "vitest": "0.34.2",
    "gh-pages": "^6.0.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ],
    "timers": "fake",
    "resetMocks": false
  }
}
